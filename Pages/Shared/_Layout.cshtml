<!DOCTYPE html>
<html lang="pt-BR">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="~/vendor/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="~/vendor/owl/css/owl.css">
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true">
  <link rel="shortcut icon" href="~/img/favicon.png" type="image/x-icon">
  <title>GRazor</title>
</head>

<body>
  <header>
    <partial name="Shared/NavMenuPartial" />
  </header>
  <main>
    <div class="container">
      @RenderBody()
    </div>
  </main>

  <footer>
    <p>2023 - &copy; GRazor</p>
  </footer>


  <script src="~/vendor/jquery/jquery-3.6.1.min.js"></script>
  <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
  <script src="~/vendor/owl/js/owl.js"></script>
  @RenderSection(name:"scripts", required: false)
